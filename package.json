{
  "name": "movemate-backend",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "tsc && tsc-alias",
    "start": "npm run build && NODE_ENV=production node dist/index.js",
    "dev": "cross-env NODE_ENV=development nodemon --inspect src/index.ts",
    "build:swc": "swc src -d dist --source-maps --copy-files",
    "lint": "npx eslint \"src/**/*.+(js|jsx|ts|tsx)\"",
    "lint:fix": "npm run lint -- --fix"
  },
  "dependencies": {
    "@prisma/client": "^5.20.0",
    "bcrypt": "^5.1.1",
    "class-validator": "^0.14.1",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^16.4.5",
    "envalid": "^8.0.0",
    "express": "~4.16.1",
    "express-session": "^1.18.1",
    "googleapis": "^144.0.0",
    "helmet": "^8.0.0",
    "jsonwebtoken": "^9.0.2",
    "passport": "^0.7.0",
    "passport-facebook": "^3.0.0",
    "passport-google-oauth20": "^2.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.12.0",
    "@swc/cli": "^0.4.1-nightly.20240914",
    "@swc/core": "^1.7.35",
    "@types/cors": "^2.8.17",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^5.0.0",
    "@types/helmet": "^4.0.0",
    "@types/node": "^22.7.5",
    "cross-env": "^7.0.3",
    "eslint": "^9.12.0",
    "globals": "^15.11.0",
    "nodemon": "^3.1.7",
    "prettier": "^3.3.3",
    "prisma": "^5.20.0",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.10",
    "tsconfig-paths": "^4.2.0",
    "tslib": "^2.7.0",
    "typescript": "5.5.4",
    "typescript-eslint": "^8.8.1"
  }
}
